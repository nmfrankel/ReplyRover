// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Log {
  id            Int       @id @default(autoincrement())
  event         String
  userID        String
  username      String
  message       String
  timestamp     DateTime @default(now())
}

